{
"update_url": "https://clients2.google.com/service/update2/crx",

   "background": {
      "page": "main.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": "assets/images/icon-blue.png",
      "default_popup": "popup.html",
      "default_title": "__MSG_manifest_iconTitle__"
   },
   "default_locale": "en",
   "description": "Proxy SwitchySharp (Fastest) chooses automatically the fastest proxy server available according to your list of proxies. Enjoy!",
   "icons": {
      "128": "assets/images/icon-128.png",
      "16": "assets/images/icon.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "18.0.0",
   "name": "Proxy SwitchySharp (Fastest Proxy Selection)",
   "options_page": "options.html",
   "permissions": [ "webRequest", "webRequestBlocking", "proxy", "tabs", "\u003Call_urls>", "http://*/*", "https://*/*", "ftp://*/*" ],
   "version": "3.0.1"
}
